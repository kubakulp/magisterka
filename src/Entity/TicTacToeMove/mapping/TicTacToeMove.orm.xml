<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                          http://raw.github.com/doctrine/doctrine2/master/doctrine-mapping.xsd">

    <entity name="App\Entity\TicTacToeMove\TicTacToeMove" table="tic_tac_toe_move">
        <id name="id" column="id" type="uuid"/>
        <field name="gameId" column="game_id" type="integer"/>
        <field name="playerId" column="player_id"/>
        <field name="playerNumber" column="player_number" type="integer"/>
        <field name="moveCount" column="move_count" type="integer"/>
        <field name="moveAnswer" column="move_answer" type="text"/>
        <field name="moveNumber" column="move_number" type="integer"/>
        <field name="moveEvaluation" column="move_evaluation" type="integer"/>
        <field name="moveWasCorrect" column="move_was_correct" type="boolean"/>
        <field name="createdAt" column="created_at" type="datetime"/>
    </entity>
</doctrine-mapping>
