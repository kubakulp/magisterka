<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                          http://raw.github.com/doctrine/doctrine2/master/doctrine-mapping.xsd">

    <entity name="App\Entity\LicitationPlayer\LicitationPlayer" table="licitation_player">
        <id name="id" column="id"/>
        <id name="licitationGameId" column="licitation_game_id" type="integer"/>
        <field name="wasEliminated" column="was_eliminated" type="boolean"/>
        <field name="playerWon" column="player_won" type="boolean"/>
    </entity>
</doctrine-mapping>
