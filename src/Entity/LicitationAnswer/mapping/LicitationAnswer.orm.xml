<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                          http://raw.github.com/doctrine/doctrine2/master/doctrine-mapping.xsd">

    <entity name="App\Entity\LicitationAnswer\LicitationAnswer" table="licitation_answer">
        <id name="licitationGameId" column="licitation_game_id" type="integer"/>
        <id name="playerId" column="player_id"/>
        <id name="moveNumber" column="move_number" type="integer"/>
        <field name="moveAnswer" column="move_answer"/>
        <field name="moveWasCorrect" column="move_was_correct" type="boolean"/>
    </entity>
</doctrine-mapping>
