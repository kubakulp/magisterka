<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                          http://raw.github.com/doctrine/doctrine2/master/doctrine-mapping.xsd">

    <entity name="App\Entity\LicitationGame\LicitationGame" table="licitation_game">
        <id name="id" column="id" type="integer">
            <generator strategy="IDENTITY"/>
        </id>
        <field name="promptType" column="prompt_type" enum-type="LicitationPromptType"/>
        <field name="numberOfRepetitions" column="number_of_repetitions" type="integer"/>
        <field name="numberOfTokens" column="number_of_tokens" type="integer"/>
    </entity>
</doctrine-mapping>
